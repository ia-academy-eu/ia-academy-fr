      <!-- grid-container -->
      <grid-container id="ScxpzGc" data-type="grid-container">
        <!-- grid-content -->
        <grid-content id="DHjWfDN" contenteditable="true" data-type="html">
          <!-- grid-box-container -->    
        
  <grid-box-container id="xNgeVgE">
    <grid-box id="HdqCpjD" contenteditable="true">
    <h1 class="grid-h1" contenteditable="true">Titre H1</h1>
  
    <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit duis at arcu sed odio aliquet molestie eget at risus.</p>
  </grid-box>
    <grid-edit-box class="grid-edit-box no-export">
      
  
    
    <div contenteditable="false" class="section-info">Box</div>
    <button contenteditable="false" class="no-export" onclick="openFlexItems('HdqCpjD')">
      +
    </button>
    <button contenteditable="false" class="delete-grid no-export" onclick="removeContent('xNgeVgE')">
      -
    </button>
    <button contenteditable="false" class="align-items-button no-export" onclick="openAlignModal('HdqCpjD')">
      ⋮
    </button>

    <div id="align-modal-HdqCpjD" class="align-modal" style="display:none">
      <div class="align-modal-content">

      <details class="toggle-details">
      <summary>Flex Direction</summary>

        <div class="flexDirection">
          
          <button data-boxid="HdqCpjD" data-category="flexDirection" data-style="row" onclick="flexDirection('HdqCpjD', 'row')">row</button>
          <button data-boxid="HdqCpjD" data-category="flexDirection" data-style="column" data-choice="selected" onclick="flexDirection('HdqCpjD', 'column')">column</button>
          <button data-boxid="HdqCpjD" data-category="flexDirection" data-style="row-reverse" onclick="flexDirection('HdqCpjD', 'row-reverse')">row-reverse</button>
          <button data-boxid="HdqCpjD" data-category="flexDirection" data-style="column-reverse" onclick="flexDirection('HdqCpjD', 'column-reverse')">column-reverse</button>
        </div>

        </details>

        <details class="toggle-details">
          <summary>Flex Wrap</summary>

        <div class="flexWrap">
          
          <button data-boxid="HdqCpjD" data-category="flexWrap" data-style="nowrap" onclick="flexWrap('HdqCpjD', 'nowrap')">nowrap</button>
          <button data-boxid="HdqCpjD" data-category="flexWrap" data-style="wrap" data-choice="selected" onclick="flexWrap('HdqCpjD', 'wrap')">wrap</button>
        </div>

        </details>

        <details class="toggle-details">
          <summary>Background Color</summary>

        <div class="flexColor">
          
          <label for="color-picker-HdqCpjD" class="color-picker-button no-export" style="background-color: #ffffff; border-radius: 50%; width: 25px; height: 25px; display: inline-block; border: 2px solid #000;">
          </label>
    
          <input type="color" id="color-picker-HdqCpjD" class="color-picker no-export" oninput="changeBoxColor('HdqCpjD', this.value)" value="#ffffff" style="display: none;">
          </div>

        </details>

        <details class="toggle-details">
            <summary>Space</summary>
            <div class="Padding">
              
              <label for="min-padding-HdqCpjD">Min:</label>
              <input type="text" id="min-padding-HdqCpjD" value="35px">

              <label for="preferred-padding-HdqCpjD">Preferred:</label>
              <input type="text" id="preferred-padding-HdqCpjD" value="3vw">

              <label for="max-padding-HdqCpjD">Max:</label>
              <input type="text" id="max-padding-HdqCpjD" value="50px">

              <button onclick="applyPadding('HdqCpjD')">Set Padding</button>
            </div>
          </details>

          <details class="toggle-details">
          <summary>Flex Properties</summary>
          <div class="FlexProperties">

            <label for="flex-basis-HdqCpjD">Flex Basis:</label>
            <input type="text" id="flex-basis-HdqCpjD" value="0">

            <label for="flex-shrink-HdqCpjD">Flex Shrink:</label>
            <input type="text" id="flex-shrink-HdqCpjD" value="1">

            <label for="flex-grow-HdqCpjD">Flex Grow:</label>
            <input type="text" id="flex-grow-HdqCpjD" value="1">

            <button onclick="setFlexProperties('HdqCpjD')">Set Flex Properties</button>
          </div>
        </details>


        <details class="toggle-details">
          <summary>Place Content</summary>

        <div class="placeContent">
          
          <button data-boxid="HdqCpjD" data-category="placeContent" data-style="start" data-choice="selected" onclick="placeContent('HdqCpjD', 'start')">start</button>
          <button data-boxid="HdqCpjD" data-category="placeContent" data-style="start center" onclick="placeContent('HdqCpjD', 'start center')">start center</button>
          <button data-boxid="HdqCpjD" data-category="placeContent" data-style="start end" onclick="placeContent('HdqCpjD', 'start end')">start end</button>
          <button data-boxid="HdqCpjD" data-category="placeContent" data-style="center start" onclick="placeContent('HdqCpjD', 'center start')">center start</button>
          <button data-boxid="HdqCpjD" data-category="placeContent" data-style="center" onclick="placeContent('HdqCpjD', 'center')">center</button>
          <button data-boxid="HdqCpjD" data-category="placeContent" data-style="center end" onclick="placeContent('HdqCpjD', 'center end')">center end</button>
          <button data-boxid="HdqCpjD" data-category="placeContent" data-style="end start" onclick="placeContent('HdqCpjD', 'end start')">end start</button>
          <button data-boxid="HdqCpjD" data-category="placeContent" data-style="end center" onclick="placeContent('HdqCpjD', 'end center')">end center</button>
          <button data-boxid="HdqCpjD" data-category="placeContent" data-style="end end" onclick="placeContent('HdqCpjD', 'end')">end</button>
        </div>
        </details>

        

        <span class="align-close" onclick="closeAlignModal('HdqCpjD')">×</span>
      </div>
    </div>
  
  
    </grid-edit-box>
