    <grid-edit-box class="grid-edit-box no-export">
      
  
    
    <div contenteditable="false" class="section-info">Box</div>
    <button contenteditable="false" class="no-export" onclick="openFlexItems('kMTBDkQ')">
      +
    </button>
    <button contenteditable="false" class="delete-grid no-export" onclick="removeContent('kqzlFvQ')">
      -
    </button>
    <button contenteditable="false" class="align-items-button no-export" onclick="openAlignModal('kMTBDkQ')">
      ⋮
    </button>

    <div id="align-modal-kMTBDkQ" class="align-modal" style="display: none;">
      <div class="align-modal-content">

      <details class="toggle-details">
      <summary>Flex Direction</summary>

        <div class="flexDirection">
          
          <button data-boxid="kMTBDkQ" data-category="flexDirection" data-style="row" onclick="flexDirection('kMTBDkQ', 'row')">row</button>
          <button data-boxid="kMTBDkQ" data-category="flexDirection" data-style="column" data-choice="selected" onclick="flexDirection('kMTBDkQ', 'column')">column</button>
          <button data-boxid="kMTBDkQ" data-category="flexDirection" data-style="row-reverse" onclick="flexDirection('kMTBDkQ', 'row-reverse')">row-reverse</button>
          <button data-boxid="kMTBDkQ" data-category="flexDirection" data-style="column-reverse" onclick="flexDirection('kMTBDkQ', 'column-reverse')">column-reverse</button>
        </div>

        </details>

        <details class="toggle-details">
          <summary>Flex Wrap</summary>

        <div class="flexWrap">
          
          <button data-boxid="kMTBDkQ" data-category="flexWrap" data-style="nowrap" onclick="flexWrap('kMTBDkQ', 'nowrap')">nowrap</button>
          <button data-boxid="kMTBDkQ" data-category="flexWrap" data-style="wrap" data-choice="selected" onclick="flexWrap('kMTBDkQ', 'wrap')">wrap</button>
        </div>

        </details>

        <details class="toggle-details">
          <summary>Background Color</summary>

        <div class="flexColor">
          
          <label for="color-picker-kMTBDkQ" class="color-picker-button no-export" style="background-color: #ffffff; border-radius: 50%; width: 25px; height: 25px; display: inline-block; border: 2px solid #000;">
          </label>
    
          <input type="color" id="color-picker-kMTBDkQ" class="color-picker no-export" oninput="changeBoxColor('kMTBDkQ', this.value)" value="#ffffff" style="display: none;">
          </div>

        </details>

        <details class="toggle-details">
            <summary>Space</summary>
            <div class="Padding">
              
              <label for="min-padding-kMTBDkQ">Min:</label>
              <input type="text" id="min-padding-kMTBDkQ" value="35px">

              <label for="preferred-padding-kMTBDkQ">Preferred:</label>
              <input type="text" id="preferred-padding-kMTBDkQ" value="3vw">

              <label for="max-padding-kMTBDkQ">Max:</label>
              <input type="text" id="max-padding-kMTBDkQ" value="50px">

              <button onclick="applyPadding('kMTBDkQ')">Set Padding</button>
            </div>
          </details>

          <details class="toggle-details">
          <summary>Flex Properties</summary>
          <div class="FlexProperties">

            <label for="flex-basis-kMTBDkQ">Flex Basis:</label>
            <input type="text" id="flex-basis-kMTBDkQ" value="0">

            <label for="flex-shrink-kMTBDkQ">Flex Shrink:</label>
            <input type="text" id="flex-shrink-kMTBDkQ" value="1">

            <label for="flex-grow-kMTBDkQ">Flex Grow:</label>
            <input type="text" id="flex-grow-kMTBDkQ" value="1">

            <button onclick="setFlexProperties('kMTBDkQ')">Set Flex Properties</button>
          </div>
        </details>


        <details class="toggle-details">
          <summary>Place Content</summary>

        <div class="placeContent">
          
          <button data-boxid="kMTBDkQ" data-category="placeContent" data-style="start" onclick="placeContent('kMTBDkQ', 'start')">start</button>
          <button data-boxid="kMTBDkQ" data-category="placeContent" data-style="start center" onclick="placeContent('kMTBDkQ', 'start center')">start center</button>
          <button data-boxid="kMTBDkQ" data-category="placeContent" data-style="start end" onclick="placeContent('kMTBDkQ', 'start end')">start end</button>
          <button data-boxid="kMTBDkQ" data-category="placeContent" data-style="center start" onclick="placeContent('kMTBDkQ', 'center start')">center start</button>
          <button data-boxid="kMTBDkQ" data-category="placeContent" data-style="center" onclick="placeContent('kMTBDkQ', 'center')" data-choice="selected">center</button>
          <button data-boxid="kMTBDkQ" data-category="placeContent" data-style="center end" onclick="placeContent('kMTBDkQ', 'center end')">center end</button>
          <button data-boxid="kMTBDkQ" data-category="placeContent" data-style="end start" onclick="placeContent('kMTBDkQ', 'end start')">end start</button>
          <button data-boxid="kMTBDkQ" data-category="placeContent" data-style="end center" onclick="placeContent('kMTBDkQ', 'end center')">end center</button>
          <button data-boxid="kMTBDkQ" data-category="placeContent" data-style="end end" onclick="placeContent('kMTBDkQ', 'end')">end</button>
        </div>
        </details>

        

        <span class="align-close" onclick="closeAlignModal('kMTBDkQ')">×</span>
      </div>
    </div>
  
  
