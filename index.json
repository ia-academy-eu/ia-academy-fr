            <input type="text" id="flex-basis-nmxLoqh" value="0">

            <label for="flex-shrink-nmxLoqh">Flex Shrink:</label>
            <input type="text" id="flex-shrink-nmxLoqh" value="1">

            <label for="flex-grow-nmxLoqh">Flex Grow:</label>
            <input type="text" id="flex-grow-nmxLoqh" value="1">

            <button onclick="setFlexProperties('nmxLoqh')">Set Flex Properties</button>
          </div>
        </details>


        <details class="toggle-details" open="">
