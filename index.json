    position: absolute;
    height: 100%;
    width: 100%;
    z-index: -1;
    
  }
  
  grid-bg img,
  grid-bg video {
    position: absolute;
    object-fit: cover;
    height: 100%;
    width: 100%;
    z-index: -1;
    display: grid;
    background-color: black;
  }

  grid-content{
    display:grid;
    padding: var(--grid-padding);
    gap: var(--grid-gap);
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
  }
  grid-container{
    display: grid;
    box-shadow: var(--grid-web-shadow);
  }
  grid-box-container{
    display: grid;
    grid-template-columns: 1fr;
    grid-template-rows: 1fr;
    box-shadow: var(--grid-web-shadow);
    border-color: #00000008;
    border-width: 1px;
    overflow: hidden;
  }
  grid-box {
    background:white;
    padding: var(--grid-padding);
    display: grid;
    position: relative;
    gap: var(--grid-gap);
    min-height:50px;

  }
  
  grid-box img,
  grid-box video{
    object-fit: cover;
    width:100%;
    height:100%;
  }
