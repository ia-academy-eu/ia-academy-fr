          <button data-boxid="nmxLoqh" data-category="placeContent" data-style="center end" onclick="placeContent('nmxLoqh', 'center end')">center end</button>
          <button data-boxid="nmxLoqh" data-category="placeContent" data-style="end start" onclick="placeContent('nmxLoqh', 'end start')">end start</button>
          <button data-boxid="nmxLoqh" data-category="placeContent" data-style="end center" onclick="placeContent('nmxLoqh', 'end center')">end center</button>
          <button data-boxid="nmxLoqh" data-category="placeContent" data-style="end end" onclick="placeContent('nmxLoqh', 'end')">end</button>
        </div>
        </details>

        

        <span class="align-close" onclick="closeAlignModal('nmxLoqh')">×</span>
